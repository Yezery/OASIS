: 25%;\n      background-color: white;\n      .AvatarBox {\n        width: 90%;\n        height: 100%;\n\n        position: relative;\n        .Inf {\n          line-height: 35px;\n          position: absolute;\n          left: 5%;\n          top: 25%;\n          .NFTName {\n            width: 100%;\n            height: auto;\n            font-weight: 800;\n            text-align: left;\n            font-size: 2vw;\n          }\n          .NFTContract {\n            margin-top: 4%;\n          }\n          .NFTSupplyer {\n            text-align: left;\n            font-size: 1.1vw;\n          }\n          .NFTInf {\n            float: left;\n            .NFTCounts {\n              margin-top: 2%;\n              text-align: left;\n              font-size: 1.1vw;\n              span {\n                margin-left: 5px;\n                margin-right: 10px;\n              }\n            }\n            .NFTdescription {\n              text-align: left;\n              margin-top: 1.5%;\n              font-size: 10vw;\n            }\n          }\n        }\n        .AvatarBorder {\n          border: 4px solid white;\n          width: 10vw;\n          height: 10vw;\n          max-height: 20vw;\n          position: absolute;\n          left: 5%;\n          top: -9vw;\n          border-radius: 16px;\n          overflow: hidden;\n          box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 5px 0px,\n            rgba(0, 0, 0, 0.1) 0px 0px 1px 0px;\n          img {\n            object-fit: fill;\n            min-width: 100%;\n            height: 100%;\n            border-radius: 20px;\n            transition: all 0.6s;\n            &:hover {\n              transform: scale(1.1);\n              transition: all 0.6s;\n            }\n          }\n        }\n      }\n    }\n    .MoreNFTInfBox {\n      width: 100%;\n      height: 100%;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      .seriesNFTBox {\n        background-color: white;\n        width: 100%;\n        height: 100%;\n      }\n    }\n  }\n}\n</style>"],"mappings":";AAmEA,SAAAA,YAAA,EAAAC,kBAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,WAAA;MACAC,WAAA;MACAC,QAAA;MACAC,OAAA;MACAC,QAAA;MACAC,MAAA;MACAC,SAAA;MACAC,WAAA;MACAC,QAAA;MACAC,SAAA;MACAC,SAAA;MACAC,eAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACA,MAAAF,KAAA;MACA;QACA,KAAAd,MAAA,SAAAiB,IAAA,CAAAC,KAAA,MAAAC,MAAA,CAAAC,KAAA,CAAAC,YAAA;QACA,KAAApB,WAAA,SAAAgB,IAAA,CAAAC,KAAA,MAAAlB,MAAA;QACAsB,OAAA,CAAAC,GAAA,MAAAtB,WAAA;QACA,KAAAG,OAAA,QAAAH,WAAA,CAAAH,IAAA;QACA,KAAAK,QAAA,WAAAH,MAAA,IAAAwB,KAAA,iBAAAxB,MAAA,IAAAwB,KAAA,CACA,EACA;QACA,KAAAnB,QAAA,QAAAJ,WAAA,CAAAwB,KAAA;QACA,KAAAjB,WAAA,QAAAP,WAAA,CAAAO,WAAA;QACA,MAAAZ,YAAA,MAAAI,MAAA,KAAA0B,IAAA,CAAAC,EAAA;UACA,KAAAzB,WAAA,GAAAyB,EAAA;QACA;QACA,WAAAzB,WAAA,CAAAc,OAAA,CACAV,MAAA,GACAsB,IAAA,GACAF,IAAA,CAAAC,EAAA;UACA,KAAArB,MAAA,GAAAqB,EAAA;QACA;QACA,WAAAzB,WAAA,CAAAc,OAAA,CACAlB,IAAA,GACA8B,IAAA,GACAF,IAAA,CAAAC,EAAA;UACA,KAAApB,SAAA,GAAAoB,EAAA;QACA;QACA,WAAAzB,WAAA,CAAAc,OAAA,CACAa,SAAA,GACAD,IAAA,GACAF,IAAA,CAAAC,EAAA;UACA,KAAAlB,QAAA,GAAAkB,EAAA;QACA;QACA,WAAAzB,WAAA,CAAAc,OAAA,CACAc,UAAA,GACAF,IAAA,GACAF,IAAA,CAAAC,EAAA;UACA,KAAAjB,SAAA,GAAAiB,EAAA;QACA;MACA,SAAAI,KAAA;QACAT,OAAA,CAAAC,GAAA,CAAAQ,KAAA;MACA;IACA;IACA,MAAAhB,iBAAA;MACA,SAAAiB,KAAA,MAAAA,KAAA,QAAAtB,SAAA,EAAAsB,KAAA;QACA,KAAA9B,WAAA,CAAAc,OAAA,CAAAiB,YAAA,CAAAD,KAAA,EAAAJ,IAAA,GAAAF,IAAA,CAAAC,EAAA;UACA,KAAAf,eAAA,CAAAsB,IAAA,CAAAP,EAAA;QACA;QACAL,OAAA,CAAAC,GAAA,MAAAX,eAAA;MAEA;IAEA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}