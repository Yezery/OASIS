n        //*********************//\n        // SaleTypeList: [],\n        mac: false,\n        windows64: false,\n        windows32: false,\n        metaMask: false,\n      };\n    },\n    mounted() {\n      this.OSnow();\n      // SaleTypeList().then((res) => {\n      //   this.SaleTypeList = res.data;\n      // });\n    },\n    methods: {\n      checkMetaMaskInstall() {\n        try {\n          window.ethereum.enable();\n          this.metaMask = true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      MMCTDis() {\n        this.$refs.MMCT.classList.remove(\"animate__fadeIn\");\n        this.$refs.MMCT.classList.add(\"animate__fadeOut\");\n        setTimeout(() => {\n          this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n        }, 750);\n      },\n      CheckConnection() {\n        if (window.ethereum && !this.isconnect) {\n          window.ethereum.enable().then((res) => {\n            if (res) {\n              this.isconnect = true;\n            }\n          });\n        } else {\n          this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n          this.$refs.MMCT.classList.remove(\"animate__fadeOut\");\n          this.$refs.MMCT.classList.add(\"animate__fadeIn\");\n          setTimeout(() => {\n            this.MMCTDis();\n          }, 1800);\n        }\n      },\n      //判断系统类型\n      OSnow() {\n        var agent = navigator.userAgent.toLowerCase();\n        var isMac = /macintosh|mac os x/i.test(navigator.userAgent);\n        if (agent.indexOf(\"win32\") >= 0 || agent.indexOf(\"wow32\") >= 0) {\n          this.windows32 = true;\n          this.checkMetaMaskInstall();\n          // https://go.microsoft.com/fwlink/?LinkID=2093505\n        }\n        if (agent.indexOf(\"win64\") >= 0 || agent.indexOf(\"wow64\") >= 0) {\n          this.windows64 = true;\n          this.checkMetaMaskInstall();\n          // https://go.microsoft.com/fwlink/?LinkID=2093437\n        }\n        if (isMac) {\n          this.mac = true;\n          this.checkMetaMaskInstall();\n        }\n      },\n    },\n  };\n</script>\n\n<style lang=\"scss\" scoped>\n.MainWindow {\n  \n  height: 100vh;\n  width: 100%;\n  overflow: auto;\n  //   & .el-input{\n  //     border-radius: 40px;\n  //   }\n  .centerBox {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n  }\n}\n// @keyframes shine {\n//   100% {\n//     box-shadow: rgb(85, 91, 255) 0px 0px 0px 4px;\n//   }\n// }\n// @keyframes textshine {\n//   100% {\n//     color: rgb(85, 91, 255);\n//   }\n// }\n// @keyframes iconshine {\n//   100% {\n//     transform: translate(4px);\n//   }\n// }\n\n.searchBox {\n  width: 100%;\n  height: 90px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  // &:focus-within i {\n  //   font-weight: 800;\n  //   transition: all 0.5s ease-in-out;\n\n  //   animation: iconshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-webkit-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-moz-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-ms-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n\n  & .search {\n    height: 60px;\n    width: 95%;\n    border-radius: 50px;\n    border: 1px solid gray;\n    background-color: var(--White--);\n    text-align: center;\n    color: var(--Dark--);\n    font-size: 17px;\n    box-shadow: var(--boxshdow-style--);\n    // &:focus {\n    //   animation: shine 1s linear 1s infinite alternate;\n    // }\n    transition: all 0.3s ease-in-out;\n    &:hover {\n      box-shadow: rgba(14, 30, 37, 0.12) 0px 2px 4px 0px,\n        rgba(14, 30, 37, 0.32) 0px 2px 16px 0px;\n      transition: all 0.3s ease-in-out;\n    }\n  }\n  & i {\n    position: absolute;\n    left: 5%;\n    top: 20px;\n    font-size: 23px;\n    color: gray;\n  }\n}\ninput::-webkit-inp