{"ast":null,"code":"import ad from \"@/views/market/ad.vue\";\nimport ad1 from \"@/views/market/ADslots/ad1.vue\";\nimport MarketSellIndex from \"@/views/market/MarketShopType/index.vue\";\nimport { search } from \"@/api/axios/ownerContractLIst\";\nexport default {\n  components: {\n    ad,\n    ad1,\n    MarketSellIndex\n  },\n  data() {\n    return {\n      SearchVo: {\n        key: \"\",\n        isActive: false,\n        minPrice: \"\",\n        maxPrice: \"\",\n        minMaxmums: \"\",\n        maxMaxmums: \"\"\n      },\n      isSearch: true,\n      SearchList: [],\n      heplerShow: false\n    };\n  },\n  mounted() {},\n  watch: {\n    SearchVo: {\n      handler(newVal) {\n        if (newVal.key == \"\") {\n          this.isSearch = true;\n        }\n      },\n      deep: true\n    }\n  },\n  methods: {\n    MMCTDis() {\n      this.$refs.MMCT.classList.remove(\"animate__fadeIn\");\n      this.$refs.MMCT.classList.add(\"animate__fadeOut\");\n      setTimeout(() => {\n        this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n      }, 750);\n    },\n    SearchNFT() {\n      this.isSearch = true;\n      search(this.SearchVo).then(re => {\n        console.log(re);\n        this.SearchList = re.data.data;\n      });\n    },\n    GETHashAvatar(address) {\n      return \"data:image/png;base64,\" + new this.Identicon(address, 120).toString();\n    }\n  }\n};","map":{"version":3,"names":["ad","ad1","MarketSellIndex","search","components","data","SearchVo","key","isActive","minPrice","maxPrice","minMaxmums","maxMaxmums","isSearch","SearchList","heplerShow","mounted","watch","handler","newVal","deep","methods","MMCTDis","$refs","MMCT","classList","remove","add","setTimeout","MetaMaskTipsIsShow","SearchNFT","then","re","console","log","GETHashAvatar","address","Identicon","toString"],"sources":["src/views/market/NFTMarket.vue"],"sourcesContent":["<template>\n  <div class=\"MainWindow animate__animated animate__fadeInDown\">\n    <!---------------->\n    <div class=\"searchBox \">\n      <span class=\"searchSpan\" style=\"width: 100%; position: relative\">\n        <i class=\"el-icon-search\"></i>\n        <input type=\"text\" class=\"search\" placeholder=\"Search any collection\" v-model=\"SearchVo.key\" @keydown.enter.prevent=\"SearchNFT\" />\n      </span>\n    </div>\n\n    <div class=\"centerBox\">\n      <div class=\"ad \">\n        <ad>\n          <template v-slot:ad1>\n            <ad1></ad1>\n          </template>\n          <template v-slot:ad2> 2</template>\n          <template v-slot:ad3> 3</template>\n          <template v-slot:ad4> 4 </template>\n        </ad>\n      </div>\n\n      <div class=\"MarketMain animate__animated animate__fadeInLeft\" style=\"animation-delay: 1s;\">\n        <div class=\"SearchBox animate__animated animate__fadeInLeft\" v-if=\"isSearch\">\n          <div class=\"NFTInf\" v-for=\"NFT in this.SearchList\" :key=\"NFT.TokenURI\">\n            <div class=\"imageBox\">\n              <img class=\"NFTImage\" :src=\"NFT.ipfsPath \" alt=\"\" />\n            </div>\n            <div class=\"Inf\">\n              <div class=\"NFTName\">\n                {{ NFT.nftName }}\n              </div>\n              <div class=\"InfBottom\">\n                <div class=\"ownerBox\">\n                  <div class=\"ownerAddress\">{{ NFT.currentowner.slice(0, 5) + \"...\" + NFT.currentowner.slice(37)}}</div>\n                  <div class=\"ownerAvatarBox\">\n                    <img class=\"owner\" :src=\"GETHashAvatar(NFT.currentowner)\" alt=\"\" width=\"35px\" height=\"35px\" />\n                  </div>\n                </div>\n                <div class=\"PriceBox\">\n                  <div class=\"PriceTitle\">\n                    Volume\n                  </div>\n                  <div style=\"font-weight: 800;\">{{ $store.state.Web3.utils.fromWei(NFT.price, 'ether') }} ETH</div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div v-else>\n  