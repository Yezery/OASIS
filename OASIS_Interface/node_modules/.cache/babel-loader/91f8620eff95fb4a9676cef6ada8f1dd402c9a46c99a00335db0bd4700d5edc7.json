{"ast":null,"code":"import { getNFTStruct } from \"@/api/axios/contract\";\nexport default {\n  name: \"NFTInf\",\n  data() {\n    return {\n      NFTInf: null,\n      NFTMetaData: null,\n      NFTContract: null,\n      supplyer: \"\",\n      NFTName: \"\",\n      NFTImage: \"#\",\n      symbol: null,\n      NFTSeries: \"\",\n      description: \"啊圣诞节啊是几点开链接啊索朗多吉空间\",\n      maximums: 0,\n      currentId: 0\n    };\n  },\n  mounted() {\n    this.init();\n  },\n  methods: {\n    async init() {\n      this.NFTInf = await JSON.parse(this.$store.state.marketNFTInf);\n      this.NFTMetaData = await JSON.parse(this.NFTInf[6]);\n      console.log(this.NFTMetaData);\n      this.NFTName = this.NFTMetaData.name;\n      this.supplyer = `${this.NFTInf[1].slice(0, 5)}...${this.NFTInf[1].slice(37)}`;\n      this.NFTImage = this.NFTMetaData.image;\n      // this.description = this.NFTMetaData.description;\n      await getNFTStruct(this.NFTInf[2]).then(re => {\n        this.NFTContract = re;\n      });\n      await this.NFTContract.methods.symbol().call().then(re => {\n        this.symbol = re;\n      });\n      await this.NFTContract.methods.name().call().then(re => {\n        this.NFTSeries = re;\n      });\n      await this.NFTContract.methods._maximums().call().then(re => {\n        this.maximums = re;\n      });\n      await this.NFTContract.methods._currentId().call().then(re => {\n        this.currentId = re;\n      });\n    }\n  }\n};","map":{"version":3,"names":["getNFTStruct","name","data","NFTInf","NFTMetaData","NFTContract","supplyer","NFTName","NFTImage","symbol","NFTSeries","description","maximums","currentId","mounted","init","methods","JSON","parse","$store","state","marketNFTInf","console","log","slice","image","then","re","call","_maximums","_currentId"],"sources":["src/views/market/NFTInf.vue"],"sourcesContent":["<template>\n  <div class=\"NFTInfMain\">\n    <div class=\"NFTInfBackground\">\n    </div>\n    <div class=\"NFTInfContextBox\">\n      <div class=\"NFTInfBox\">\n        <div class=\"AvatarBox\">\n          <div class=\"AvatarBorder\">\n            <img :src=\"NFTImage\" alt=\"\">\n          </div>\n          <div class=\"Inf\">\n            <div class=\"NFTName\">\n              <h1>{{NFTName}}</h1>\n            </div>\n            <div class=\"NFTContract\">\n              <div class=\"NFTSupplyer\">\n                由 <span style=\"font-weight: 800;font-size: 1.2vw;\">{{supplyer}}</span>\n              </div>\n              <div class=\"NFTInf\">\n                <div class=\"NFTCounts\">\n                  系列<span style=\"font-weight: 800;\">{{ NFTSeries }}</span> ·\n                  代币符号<span style=\"font-weight: 800;\">{{ symbol }}</span> ·\n                  限量铸造<span style=\"font-weight: 800;\">{{ maximums }}</span> ·\n                  目前已铸造数量<span style=\"font-weight: 800;\">{{ currentId }}</span>\n                  \n                </div>\n                <div class=\"NFTdescription\">\n                  {{ description }}\n                </div>\n              </div>\n            </div>\n            <!-- <h3>lskajdklasjd</h3>\n          <h3>lskajdklasjd</h3>\n          <h3>lskajdklasjd</h3>\n          <h3>lskajdklasjd</h3> -->\n          </div>\n        </div>\n\n      </div>\n      <div class=\"MoreNFTInfBox\"></div>\n    </div>\n    {{ NFTInf }}\n  </div>\n</template>\n\n<script>\n  import { getNFTStruct } from \"@/api/axios/contract\";\n  export default {\n    name: \"NFTInf\",\n    data() {\n      return {\n        NFTInf: null,\n        NFTMetaData: null,\n        NFTContract: null,\n        supplyer: \"\",\n        NFTName: \"\",\n        NFTImage: \"#\",\n        symbol: null,\n        NFTSeries: \"\",\n        description: \"啊圣诞节啊是几点开链接啊索朗多吉空间\",\n        maximums: 0,\n        currentId:0\n      };\n    },\n    mounted() {\n      this.init();\n    },\n    methods: {\n      async init() {\n        this.NFTInf = await JSON.parse(this.$store.state.marketNFTInf);\n        this.NFTMetaData = await JSON.parse(this.NFTInf[6]);\n        console.log(this.NFTMetaData);\n        this.NFTName = this.NFTMetaData.name;\n        this.supplyer = `${this.NFTInf[1].slice(0, 5)}...${this.NFTInf[1].slice(\n          37\n        )}`;\n        this.NFTImage = this.NFTMetaData.image;\n        // this.description = this.NFTMetaData.description;\n        await getNFTStruct(this.NFTInf[2]).then((re) => {\n          this.NFTContract = re;\n        });\n        await this.NFTContract.methods.symbol().call().then(re => { this.symbol = re })\n        await this.NFTContract.methods.name().call().then(re => { this.NFTSeries = re })\n        await this.NFTContract.methods._maximums().call().then(re => { this.maximums = re })\n        await this.NFTContract.methods._currentId().call().then(re => { this.currentId = re })\n        \n      },\n    },\n  };\n</script>\n\n<style lang=\"scss\" scoped>\n.NFTInfMain {\n  font-family: Arial, Helvetica, sans-serif;\n  min-width: min-content;\n  width: 100%;\n  height: 100%;\n  .NFTInfBackground {\n    width: 100%;\n    height: 250px;\n    background-image: url(\"@/assets/webAssets/MetaMask.png\");\n    background-repeat: repeat;\n    background-size: contain;\n  }\n  .NFTInfContextBox {\n    width: 100%;\n    height: 100%;\n    .NFTInfBox {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      width: 100%;\n      height: 200px;\n      .AvatarBox {\n        width: 100%;\n        height: 100%;\n        background-color: white;\n        position: relative;\n        .Inf {\n          line-height: 28px;\n          position: absolute;\n          left: 5%;\n          top: 25%;\n          .NFTName {\n            width: 100%;\n            height: auto;\n            font-weight: 800;\n            text-align: left;\n            font-size: 2vw;\n          }\n          .NFTContract{\n            margin-top: 4%;\n          }\n          .NFTSupplyer {\n            text-align: left;\n            font-size: 1.1vw;\n          }\n          .NFTInf {\n            float: left;\n            .NFTCounts {\n              text-align: left;\n              font-size: 1.1vw;\n              span{\n                margin-left: 5px;\n                margin-right: 10px;\n              }\n            }\n            .NFTdescription{\n              text-align: left;\n              font-size: 0.8vw;\n            }\n          }\n        }\n        .AvatarBorder {\n          border: 4px solid white;\n          width: 10vw;\n          height: 10vw;\n          max-height: 20vw;\n          position: absolute;\n          left: 5%;\n          top: -9vw;\n          border-radius: 16px;\n          overflow: hidden;\n          box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 5px 0px,\n            rgba(0, 0, 0, 0.1) 0px 0px 1px 0px;\n          img {\n            object-fit:fill;\n            min-width: 100%;\n            height: 100%;\n            border-radius: 20px;\n            transition: all 0.6s;\n            &:hover {\n              transform: scale(1.1);\n              transition: all 0.6s;\n            }\n          }\n        }\n      }\n    }\n    .MoreNFTInfBox {\n      width: 100%;\n      height: 100%;\n      // background-color: yellow;\n    }\n  }\n}\n</style>"],"mappings":"AA8CA,SAAAA,YAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,WAAA;MACAC,WAAA;MACAC,QAAA;MACAC,OAAA;MACAC,QAAA;MACAC,MAAA;MACAC,SAAA;MACAC,WAAA;MACAC,QAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACA,MAAAD,KAAA;MACA,KAAAZ,MAAA,SAAAc,IAAA,CAAAC,KAAA,MAAAC,MAAA,CAAAC,KAAA,CAAAC,YAAA;MACA,KAAAjB,WAAA,SAAAa,IAAA,CAAAC,KAAA,MAAAf,MAAA;MACAmB,OAAA,CAAAC,GAAA,MAAAnB,WAAA;MACA,KAAAG,OAAA,QAAAH,WAAA,CAAAH,IAAA;MACA,KAAAK,QAAA,WAAAH,MAAA,IAAAqB,KAAA,iBAAArB,MAAA,IAAAqB,KAAA,CACA,EACA;MACA,KAAAhB,QAAA,QAAAJ,WAAA,CAAAqB,KAAA;MACA;MACA,MAAAzB,YAAA,MAAAG,MAAA,KAAAuB,IAAA,CAAAC,EAAA;QACA,KAAAtB,WAAA,GAAAsB,EAAA;MACA;MACA,WAAAtB,WAAA,CAAAW,OAAA,CAAAP,MAAA,GAAAmB,IAAA,GAAAF,IAAA,CAAAC,EAAA;QAAA,KAAAlB,MAAA,GAAAkB,EAAA;MAAA;MACA,WAAAtB,W