{"ast":null,"code":"import ad from \"@/views/market/ad.vue\";\nimport ad1 from \"@/views/market/ADslots/ad1.vue\";\nimport MarketSellIndex from \"@/views/market/MarketShopType/index.vue\";\nexport default {\n  components: {\n    ad,\n    ad1,\n    MarketSellIndex\n  },\n  data() {\n    return {\n      activeName: \"Art\",\n      //****钱包连接弹出框******//\n      MetaMaskTipsIsShow: false,\n      isDisable: false\n      //*********************//\n      // SaleTypeList: [],\n    };\n  },\n\n  mounted() {\n    SaleTypeList().then(res => {\n      this.SaleTypeList = res.data;\n    });\n  },\n  methods: {\n    MMCTDis() {\n      this.$refs.MMCT.classList.remove(\"animate__fadeIn\");\n      this.$refs.MMCT.classList.add(\"animate__fadeOut\");\n      setTimeout(() => {\n        this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n      }, 750);\n    },\n    CheckConnection() {\n      if (window.ethereum && !this.isconnect) {\n        window.ethereum.enable().then(res => {\n          if (res) {\n            this.isconnect = true;\n          }\n        });\n      } else {\n        this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n        this.$refs.MMCT.classList.remove(\"animate__fadeOut\");\n        this.$refs.MMCT.classList.add(\"animate__fadeIn\");\n        setTimeout(() => {\n          this.MMCTDis();\n        }, 1800);\n      }\n    }\n  }\n};","map":{"version":3,"names":["ad","ad1","MarketSellIndex","components","data","activeName","MetaMaskTipsIsShow","isDisable","mounted","SaleTypeList","then","res","methods","MMCTDis","$refs","MMCT","classList","remove","add","setTimeout","CheckConnection","window","ethereum","isconnect","enable"],"sources":["src/views/market/NFTMarket.vue"],"sourcesContent":["<template>\n  <div class=\"MainWindow\">\n    <!---------------->\n    <div class=\"searchBox\">\n      <span style=\"width: 100%; position: relative\">\n        <i class=\"el-icon-search\"></i>\n        <input type=\"text\" class=\"search\" placeholder=\"Search any collection\" />\n      </span>\n    </div>\n    <div class=\"centerBox\">\n      <div class=\"ad animate__animated animate__fadeInDown\">\n        <ad>\n          <template v-slot:ad1>\n            <ad1></ad1>\n          </template>\n          <template v-slot:ad2> 2</template>\n          <template v-slot:ad3> 3</template>\n          <template v-slot:ad4> 4 </template>\n        </ad>\n      </div>\n      <div class=\"SellTitle \">\n        <h3 class=\"title animate__animated animate__fadeInLeft\" style=\"animation-delay: 1.1s;\">Trending NFTs</h3>\n        <!-- <div class=\"SellAll animate__animated animate__fadeInRight\" style=\"animation-delay: 1.2s;\">\n        </div> -->\n      </div>\n      <div class=\"MarketMain animate__animated animate__fadeInDown\" style=\"animation-delay: 1s;\">\n        <el-tabs v-model=\"activeName\">\n          <template v-for=\"SaleType in SaleTypeList.data\">\n            <el-tab-pane :name=\"SaleType.Type\" :key=\"SaleType.Id\">\n              <template slot=\"label\">\n                <span class=\"TypeSelect\">\n                  <span class=\"NFTTypeIcon\"><img :src=\"SaleType.Icon\" alt=\"\" width=\"24px\" height=\"24px\" /></span>\n                  {{ SaleType.Type }}\n                </span>\n              </template>\n           \n            </el-tab-pane>\n          </template>\n        </el-tabs>\n        <MarketSellIndex ></MarketSellIndex>\n       \n    </div>\n  </div>\n\n  <!-- 钱包连接提醒 -->\n  <div class=\"Mask animate__animated animate__fadeIn\" v-show=\"MetaMaskTipsIsShow\" ref=\"MMCT\">\n    <div class=\"MetaMaskConnectionTip\">\n      <div class=\"MetaMaskbox\">\n        <div class=\"BOX\">\n          <div class=\"MetaMaskAvatar\">\n            <img src=\"@/assets/webAssets/MetaMask.png\" alt=\"\" width=\"100px\" height=\"100px\" style=\"object-fit: contain\" />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  </div>\n</template>\n\n<script>\n  import ad from \"@/views/market/ad.vue\";\n  import ad1 from \"@/views/market/ADslots/ad1.vue\";\n  import MarketSellIndex from \"@/views/market/MarketShopType/index.vue\";\n  export default {\n    components: { ad, ad1, MarketSellIndex },\n    data() {\n      return {\n        activeName: \"Art\",\n        //****钱包连接弹出框******//\n        MetaMaskTipsIsShow: false,\n        isDisable: false,\n        //*********************//\n        // SaleTypeList: [],\n\n      };\n    },\n    mounted() {\n\n      SaleTypeList().then((res) => {\n        this.SaleTypeList = res.data;\n      });\n    },\n    methods: {\n      MMCTDis() {\n        this.$refs.MMCT.classList.remove(\"animate__fadeIn\");\n        this.$refs.MMCT.classList.add(\"animate__fadeOut\");\n        setTimeout(() => {\n          this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n        }, 750);\n      },\n      CheckConnection() {\n        if (window.ethereum && !this.isconnect) {\n          window.ethereum.enable().then((res) => {\n            if (res) {\n              this.isconnect = true;\n            }\n          });\n        } else {\n          this.MetaMaskTipsIsShow = !this.MetaMaskTipsIsShow;\n          this.$refs.MMCT.classList.remove(\"animate__fadeOut\");\n          this.$refs.MMCT.classList.add(\"animate__fadeIn\");\n          setTimeout(() => {\n            this.MMCTDis();\n          }, 1800);\n        }\n      },\n\n    },\n  };\n</script>\n\n<style lang=\"scss\" scoped>\n.MainWindow {\n  \n  height: 100vh;\n  width: 100%;\n  overflow: auto;\n  //   & .el-input{\n  //     border-radius: 40px;\n  //   }\n  .centerBox {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n  }\n}\n// @keyframes shine {\n//   100% {\n//     box-shadow: rgb(85, 91, 255) 0px 0px 0px 4px;\n//   }\n// }\n// @keyframes textshine {\n//   100% {\n//     color: rgb(85, 91, 255);\n//   }\n// }\n// @keyframes iconshine {\n//   100% {\n//     transform: translate(4px);\n//   }\n// }\n\n.searchBox {\n  width: 100%;\n  height: 90px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  // &:focus-within i {\n  //   font-weight: 800;\n  //   transition: all 0.5s ease-in-out;\n\n  //   animation: iconshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-webkit-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-moz-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n  // &:focus-within ::-ms-input-placeholder {\n  //   animation: textshine 1s linear 1s infinite alternate;\n  // }\n\n  & .search {\n    height: 60px;\n    width: 95%;\n    border-radius: 50px;\n    border: 1px solid gray;\n    background-color: var(--White--);\n    text-align: center;\n    color: var(--Dark--);\n    font-size: 17px;\n    box-shadow: var(--boxshdow-style--);\n    // &:focus {\n    //   animation: shine 1s linear 1s infinite alternate;\n    // }\n    transition: all 0.3s ease-in-out;\n    &:hover {\n      box-shadow: rgba(14, 30, 37, 0.12) 0px 2px 4px 0px,\n        rgba(14, 30, 37, 0.32) 0px 2px 16px 0px;\n      transition: all 0.3s ease-in-out;\n    }\n  }\n  & i {\n    position: absolute;\n    left: 5%;\n    top: 20px;\n    font-size: 23px;\n    color: gray;\n  }\n}\ninput::-webkit-input-placeholder {\n  /* WebKit browsers */\n  font-size: 19px;\n}\n.ad {\n  margin-top: 1.3%;\n  width: 95%;\n  height: 350px;\n  overflow: hidden;\n  border-radius: 38px;\n  position: relative;\n  z-index: 1;\n}\n.SellTitle {\n  width: 95%;\n  height: 40px;\n  margin-top: 2vh;\n  margin-bottom: 2vh;\n  font-size: 1vw;\n  text-align: center;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  & .SellAll a {\n    font-size: 1vw;\n    color: #55c960;\n    padding: 5px;\n    width: 20px;\n  }\n  & .title {\n    font-weight: 800;\n    font-family: \"Gilroy-Medium\";\n    font-size: 1.7vw;\n    color: var(--Dark--);\n    width: 13vw;\n    float: left;\n    transition: all 0.8s ease-in-out;\n  }\n}\n.MarketMain {\n  width: 90%;\n}\n.TypeSelect {\n  font-size: 1.15vw;\n  font-family: \"Transformers_Movie\";\n\n  // background-color: var(--White--);\n  box-shadow: var(--boxshdow-style--);\n  //   &:hover {\n  //     background-color: var(--Dark--);\n  //     transition: all 0.7s;\n  // }\n  //   &:hover span{\n  //     background: rgba(255, 255, 255, 0);\n  //   }\n}\n.NFTTypeIcon {\n  // padding: 17px 14px 17px 14px;\n\n  margin-right: 0.5vw;\n  & img {\n    transform: translateY(6px);\n  }\n}\n</style>\n<style>\n.el-carousel__item:nth-child(2n) {\n  background-color: #99a9bf;\n}\n\n.el-carousel__item:nth-child(2n + 1) {\n  background-color: #d3dce6;\n}\n</style>\n<style lang=\"scss\">\n::-webkit-scrollbar {\n  width: 6px;\n  height: 8px;\n  display: none;\n  background-color: transparent;\n}\n::-webkit-scrollbar-thumb {\n  background-color: #ccc;\n  border-radius: 25px;\n}\n.MarketMain .el-tabs__item {\n  width: 10vw !important;\n  border-radius: 15px !important;\n  padding: 0 !important;\n  box-shadow: none !important;\n  background-color: var(--White--) !important;\n  transition: all 0.3s ease-in-out;\n  &:hover {\n    box-shadow: rgba(0, 0, 0, 0.07) 0px 1px 1px, rgba(0, 0, 0, 0.07) 0px 2px 2px,\n      rgba(0, 0, 0, 0.07) 0px 3px 3px, rgba(0, 0, 0, 0.07) 0px 3px 3px,\n      rgba(0, 0, 0, 0.07) 0px 16px 16px !important;\n    transition: all 0.3s ease-in-out !important;\n  }\n}\n// @keyframes NftChoose {\n//     0% {\n//         border: 2px solid #40a0ff00;\n//     }\n//     100% {\n//         border: 2px solid #409eff;\n//     }\n// }\n.MarketMain .el-tabs__item.is-active {\n  border: 2px solid var(--Selected-button1-blue-black) !important;\n}\n.MarketMain .el-tabs__nav {\n  height: 55px;\n}\n</style>\n<style lang=\"scss\" scoped>\n/* MetaMask弹出框 */\n.MetaMaskConnectionTip {\n  font-family: Arial, Helvetica, sans-serif;\n  overflow: hidden;\n  position: absolute;\n  width: 350px;\n  height: 300px;\n  background-color: var(--White--);\n  color: var(--Dark--);\n  top: 50% !important;\n  left: 50% !important;\n  transform: translate(-50%, 100%) !important;\n  border-radius: 20px;\n  box-shadow: rgba(17, 17, 26, 0.05) 0px 1px 0px,\n    rgba(17, 17, 26, 0.1) 0px 0px 8px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.MetaMaskTipsBtn {\n  border: 0px solid;\n  background-color: white;\n  box-shadow: rgba(17, 17, 26, 0.05) 0px 1px 0px,\n    rgba(17, 17, 26, 0.1) 0px 0px 8px;\n  border-radius: 7px;\n  padding-top: 5%;\n  padding-bottom: 5%;\n  padding-left: 22%;\n  padding-right: 22%;\n  font-size: 13px;\n  font-weight: 500;\n}\n.MetaMaskTipsBtn:hover {\n  background-color: rgb(252, 251, 251);\n}\n.MetaMaskTips {\n  font-size: 17px;\n  font-weight: 500;\n}\n/* 遮罩层 */\n.Mask {\n  z-index: 100;\n  position: fixed;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  top: 0;\n  // background: rgba(0, 0, 0, 0.05);\n}\n</style>\n\n\n"],"mappings":"AA4DA,OAAAA,EAAA;AACA,OAAAC,GAAA;AACA,OAAAC,eAAA;AACA;EACAC,UAAA;IAAAH,EAAA;IAAAC,GAAA;IAAAC;EAAA;EACAE,KAAA;IACA;MACAC,UAAA;MACA;MACAC,kBAAA;MACAC,SAAA;MACA;MACA;IAEA;EACA;;EACAC,QAAA;IAEAC,YAAA,GAAAC,IAAA,CAAAC,GAAA;MACA,KAAAF,YAAA,GAAAE,GAAA,CAAAP,IAAA;IACA;EACA;EACAQ,OAAA;IACAC,QAAA;MACA,KAAAC,KAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAC,MAAA;MACA,KAAAH,KAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAE,GAAA;MACAC,UAAA;QACA,KAAAb,kBAAA,SAAAA,kBAAA;MACA;IACA;IACAc,gBAAA;MACA,IAAAC,MAAA,CAAAC,QAAA,UAAAC,SAAA;QACAF,MAAA,CAAAC,QAAA,CAAAE,MAAA,GAAAd,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA;YACA,KAAAY,SAAA;UACA;QACA;MACA;QACA,KAAAjB,kBAAA,SAAAA,kBAAA;QACA,KAAAQ,KAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAC,MAAA;QACA,KAAAH,KAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAE,GAAA;QACAC,UAAA;UACA,KAAAN,OAAA;QACA;MACA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}