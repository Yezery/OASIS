#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../.store/@tensorflow+tfjs@3.18.0/node_modules/@tensorflow"
else
  export NODE_PATH="$NODE_PATH:$basedir/../.store/@tensorflow+tfjs@3.18.0/node_modules/@tensorflow"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@tensorflow+tfjs@3.18.0/node_modules/@tensorflow/tfjs/dist/tools/custom_module/cli.js" "$@"
else
  exec node  "$basedir/../.store/@tensorflow+tfjs@3.18.0/node_modules/@tensorflow/tfjs/dist/tools/custom_module/cli.js" "$@"
fi
