#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../../../../../../webpack-bundle-analyzer@4.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/../../../../../../webpack-bundle-analyzer@4.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack-bundle-analyzer@4.9.1/node_modules/webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
else
  exec node  "$basedir/../../../../../../webpack-bundle-analyzer@4.9.1/node_modules/webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
fi
