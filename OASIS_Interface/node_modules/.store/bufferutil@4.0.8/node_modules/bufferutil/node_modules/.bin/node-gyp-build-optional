#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../../../../../node-gyp-build@4.6.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/../../../../../node-gyp-build@4.6.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp-build@4.6.1/node_modules/node-gyp-build/optional.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp-build@4.6.1/node_modules/node-gyp-build/optional.js" "$@"
fi
